#include <stdio.h>
/**
*  main - printif the number is postive, zero or negative
*
* Description: using the main function
* this program prints "Programming is postive, negative or zero
* Return: 0
*/
int main(void)
{
char ch;
for (ch = 'a' ; ch <= 'z' ; ch++)
{
        putchar(ch);
}
putchar('\n');
return (0);
}

